<div id="add-place" class="modal">
    <div class="modal-content">
        <h4>Adicione um lugar!</h4>

        <div class="row">
            <form class="col s12">
                <div class="row">
                    <div class="input-field col s6">
                        <input placeholder="Rua XV de Novembro, 52, Curitiba" id="address" type="text" class="validate">
                        <label for="address">Address</label>
                    </div>
                    <div class="input-field col s6">
                        <button class="btn waves-effect waves-light" type="submit" name="add_address_submit">Submit
                            <i class="material-icons">send</i>
                        </button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
